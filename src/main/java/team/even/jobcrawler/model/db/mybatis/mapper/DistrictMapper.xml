<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.even.jobcrawler.model.db.mybatis.mapper.DistrictMapper">
	<insert id="insertDistrict" parameterType="District" >
		insert into district values (#{district})
	</insert>
	
	<select id="selectAll" resultType="District">
		select * from district;
	</select>
</mapper>